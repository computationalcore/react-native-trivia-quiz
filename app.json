{
  "expo": {
    "name": "React Trivia Game",
    "slug": "react-native-trivia-quiz",
    "privacy": "public",
    "sdkVersion": "42.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.4.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vinbusquet.react-trivia-game"
    },
    "android": {
      "package": "com.vinbusquet.triviagame"
    }
  }
}